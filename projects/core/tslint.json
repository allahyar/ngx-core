{
	"extends": "../../tslint.json",
	"rules": {
		"directive-selector": [
			true,
			"attribute",
			"lib",
			"camelCase"
		],
		"component-selector": [
			true,
			"element",
			"lib",
			"kebab-case"
		],
		"align": [
			false,
			"parameters",
			"statements",
			"members",
			"arguments"
		]
	}
}
